 function generatePhoneNumber() {
     const prefixes = ['05', '06', '07'
    ];
     const prefix = prefixes[Math.floor(Math.random() * prefixes.length)
    ];
     const remainingDigits = Math.random().toString().slice(2,
    10);
     return prefix + remainingDigits;
}
